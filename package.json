{
  "name": "treepro-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "npm run dev --prefix backend",
    "build": "vite build",
    "preview": "vite preview",
    "install:all": "pnpm install && cd backend && pnpm install && cd ..",
    "build:production": "pnpm install && cd backend && pnpm install && cd .. && pnpm run build && cp -r dist/* backend/public/",
    "db:push": "npm run db:push --prefix backend"
  },
  "dependencies": {
    "@google/genai": "^1.22.0",
    "@types/express-session": "^1.18.2",
    "@types/passport": "^1.0.17",
    "chromadb": "^3.1.0",
    "concurrently": "^9.2.1",
    "connect-pg-simple": "^10.0.0",
    "express-session": "^1.18.2",
    "lucide-react": "^0.553.0",
    "memoizee": "^0.4.17",
    "openid-client": "^6.8.1",
    "passport": "^0.7.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@dyad-sh/react-vite-component-tagger": "^0.8.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}